<div class="row">
  <div class="col-12">

    <h2>Blinged-Out Artisanal Paleo Gluten-free form_for Form - for the hipster in you</h2>

    <%= form_for @user do |f| %>
      <%= f.label :username, "User name:" %>
      <%= f.text_field :username %>

      <%= f.label :password, "Password:" %>
      <%= f.password_field :password %>

      <%= f.label :email, "Email:" %>
      <%= f.email_field :email %>

      <%= f.submit "Submit", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
