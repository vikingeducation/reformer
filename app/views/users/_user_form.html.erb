<%= form_for @user do |f| %>
    <%= field_with_errors(@user, :username) %>
	<%= f.label :username, 'Yo usa name' %>
	<%= f.text_field :username, {value: 'helloworld' } %>
	<br>

	<%= field_with_errors(@user, :email) %>
	<%= f.label :email%>
	<%= f.text_field :email %>
	<br>

	<%= f.label :password %>
	<%= field_with_errors(@user, :password) %>
	<%= f.password_field :password %>

	<%=  f.submit 'hello' %>
<% end %>
