<h3>All Users:</h3>
<table>
  <thead>
    <th>Username: </th>
    <th>Email: </th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user[:username] %></td>
      <td><%= user[:email] %></td>
      <td><%= link_to "See More", user_path(user)%></td>
      <td></td>
      <% end %>
    </tr>
  </tbody>
</table>
<%= link_to "Creat New User", new_user_path, class: "btn btn-primary" %>
