<h1> All Users </h1>
<table>
	<tr>
		<td>Username</td>
		<td>Email</td>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.username %><td>
		<td><%= user.email %></td>
		<td><%= link_to "Show", user_path(user.id) %></td>
		<td><%= link_to "Edit", edit_user_path(user.id) %></td>
	</tr>
<% end %>
</table>
<%= link_to "New", new_user_path %>