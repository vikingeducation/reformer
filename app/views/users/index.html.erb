<h1> All Users </h1>
<table>
  <tr>
    <th>Email</th>
    <th>Username</th>
    <th>Password</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.password %></td>
    <td><%= link_to 'Edit', edit_user_path(user.id) %></td>
  </tr>
  <% end %>
</table>
