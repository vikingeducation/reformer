<%= form_for @user, url: {action: "update"} do |f| %>
  Username:
  <%= f.text_field :username, placeholder: "example" %>
  Email:
  <%= f.text_field :email, placeholder: "example@example.com" %>
  Password:
  <%= f.password_field :password, placeholder: "qwer12313425@w4" %>
  <%= f.submit "Update"%>
<% end %>
