<%= form_tag(users_path) do %>
 <%= label_tag :user_username, "Username" %>
 <%= text_field_tag "user[username]" %>
 <%= label_tag :user_email, "Email" %>
 <%= text_field_tag "user[email]" %>
 <%= label_tag :user_password, "Password" %>
 <%= password_field_tag "user[password]" %>
 <%= submit_tag("Submit") %>
<% end %>
